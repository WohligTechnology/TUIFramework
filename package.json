{
    "name": "TUI",
    "version": "0.1.0",
    "description": "TUI Framework",
    "main": "index.js",
    "scripts": {
        "test": "tui"
    },
    "bin": {
        "tui": "./index.js"
    },
    "author": "jagruti.shah@tui.com",
    "license": "ISC",
    "dependencies": {
        "async": "^2.5.0",
        "chalk": "^2.1.0",
        "commander": "^2.9.0",
        "delete": "^0.3.0",
        "download-github-repo": "^0.1.3",
        "lodash": "^4.15.0",
        "mv": "^2.1.1"
    },
    "prettier": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false,
        "arrowParens": "always"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module"
        },
        "rules": {
            "no-undef": 1,
            "no-unused-vars": 1,
            "indent": [
                "error",
                4
            ],
            "linebreak-style": [
                "error",
                "unix"
            ],
            "quotes": [
                "error",
                "double"
            ],
            "semi": [
                "error",
                "never"
            ]
        },
        "globals": {
            "Router": true,
            "global": true,
            "app": true,
            "ajv": true,
            "_": true,
            "ValidateRequest": true,
            "BookingModel": true,
            "UserModel": true,
            "mongoose": true,
            "Schema": true,
            "Booking": true,
            "cognitoAuthentication": true,
            "User": true
        }
    }
}
